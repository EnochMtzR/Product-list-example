{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "member-access": false,
    "arrow-parens": false,
    "trailing-comma": false,
    "curly": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
